<?xml version="1.0" encoding="utf-8" ?>
<Silk>
	<Prefixes>
	</Prefixes>

	<DataSources>
		<DataSource id="usaspending" type="file">
			<Param name="file" value="w:\dip\dedup\output1" />
			<Param name="format" value="TURTLE" />
		</DataSource>
		<!--<DataSource id="usaspending2" type="file">
			<Param name="file" value="w:\dip\output2" />
